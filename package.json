{
  "name": "hotel-booking",
  "version": "1.0.0",
  "description": "FSD lvl2 Hotel Booking ",
  "main": "index.js",
  "scripts": {
    "cleanDist": "rimraf dist/*",
    "optimizeIcons": "node optimize-icons.js",
    "prebuildSprite": "npm run cleanDist && npm run optimizeIcons",
    "buildSprite": "svgstore -o src/assets/sprites/icons-sprite.svg dist/icons/**/*.svg --inline",
    "startUI": "parcel src/uikit.pug --open",
    "build": "parcel build src/index.pug"
  },
  "author": "Roman Korenev",
  "license": "ISC",
  "dependencies": {
    "parcel-bundler": "^1.12.3",
    "sass": "^1.19.0"
  },
  "devDependencies": {
    "autoprefixer": "^9.5.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-prettier": "^3.0.1",
    "glob": "^7.1.3",
    "imagemin": "^6.1.0",
    "imagemin-svgo": "^7.0.0",
    "prettier": "^1.17.0",
    "pug": "^2.0.3",
    "rimraf": "^2.6.3",
    "svgstore-cli": "^1.3.1"
  }
}
