{
  "name": "hotel-booking",
  "version": "1.0.0",
  "description": "FSD lvl2 Hotel Booking ",
  "scripts": {
    "cleanDist": "rimraf dist/*",
    "cleanDocs": "rimraf docs/*",
    "optimizeIcons": "node optimize-icons.js",
    "prebuildSprite": "npm run cleanDist && npm run optimizeIcons",
    "buildSprite": "svgstore -o src/assets/sprites/icons-sprite.svg dist/icons/**/*.svg --inline",
    "build": "npm run cleanDist && parcel build src/index.pug",
    "start": "parcel src/index.pug --no-hmr",
    "docs": "npm run cleanDocs && parcel build src/index.pug --out-dir docs --public-url /hotel-booking-template",
    "lint:style": "stylelint 'src/**/*.scss'",
    "lint:style-fix": "stylelint 'src/**/*.scss' --fix",
    "lint:js": "eslint src/",
    "lint:js-fix": "eslint --fix src/"
  },
  "author": "Roman Korenev",
  "license": "ISC",
  "dependencies": {
    "flatpickr": "^4.6.3",
    "hyperform": "^0.10.2",
    "imask": "^5.2.1",
    "nouislider": "^13.1.5",
    "swiper": "^4.5.1",
    "timeago.js": "^4.0.2",
    "wnumb": "^1.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@romengrus/parcel-plugin-pug": "git+https://github.com/romengrus/parcel-plugin-pug.git",
    "autoprefixer": "^9.7.4",
    "browserlist": "^1.0.1",
    "caniuse-lite": "^1.0.30001025",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "imagemin": "^6.1.0",
    "imagemin-svgo": "^7.0.0",
    "parcel": "^1.12.4",
    "postcss-plugin-px2rem": "^0.8.1",
    "prettier": "^1.19.1",
    "pug": "^2.0.4",
    "rimraf": "^2.7.1",
    "sass": "^1.25.0",
    "stylelint": "^12.0.1",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-order": "^3.1.1",
    "stylelint-scss": "^3.14.2",
    "svgstore-cli": "^1.3.1"
  }
}
