    "imask": "^5.1.1",
    "lodash": "^4.17.11",
    "nouislider": "^13.1.5",
    "sass": "^1.19.0",
    "swiper": "^4.5.0",
    "timeago.js": "^4.0.0-beta.2",
    "uuid": "^3.3.2",
    "wnumb": "^1.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@types/jest": "^24.0.12",
    "@types/pikaday": "^1.7.4",
    "autoprefixer": "^9.5.1",
    "babel-jest": "^24.7.1",
